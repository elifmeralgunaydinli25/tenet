{
  "name": "tenet",
  "version": "0.0.1",
  "description": "Meta-internet",
  "main": "dest/index.js",
  "files": [
    "api",
    "prisma",
    "ui",
    "stories",
    "pages"
  ],
  "scripts": {
    "preapi:dev": "npm run migrate:test",
    "api:dev": "NODE_ENV=development nodemon --exec 'cd server && npx tsc && node dist/server/index.js' --watch ./server/ --ignore ./server/dist/ --ext ts,tsx,js,jsx",
    "api": "NODE_ENV=production cd server && node dist/server/index.js",
    "dev": "NODE_ENV=development next dev",
    "build": "NODE_ENV=production cd server && tsc",
    "migrate:test": "prisma migrate dev --name init && npm run seed:test || exit 0",
    "seed:test": "prisma db seed",
    "start": "NODE_ENV=production next start",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint . --ext ts,tsx --fix",
    "postinstall": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minamorl/tenet.git"
  },
  "author": "minamorl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/minamorl/tenet/issues"
  },
  "homepage": "https://github.com/minamorl/tenet#readme",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@fontsource/roboto": "^4.2.2",
    "@heroicons/react": "^1.0.4",
    "@storybook/addon-actions": "^6.2.6",
    "@storybook/addon-essentials": "^6.2.6",
    "@storybook/addon-links": "^6.2.6",
    "@storybook/react": "^6.2.6",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-proxy-middleware": "^1.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "postcss": "^8.3.8",
    "prettier": "^2.2.1",
    "storybook-addon-next-router": "^3.0.7",
    "tailwindcss": "^2.2.16",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.5.0",
    "@prisma/client": "^3.1.1",
    "autoprefixer": "^10.3.6",
    "apollo-server": "^2.22.2",
    "dotenv": "^10.0.0",
    "next": "^10.1.3",
    "jsonwebtoken": "^8.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "graphql": "^15.5.0",
    "mobx": "^6.2.0",
    "mobx-react": "^7.1.0",
    "prisma": "^3.1.1",
    "date-fns": "^2.25.0",
    "storybook": "^6.2.8"
  },
  "engines": {
    "node": "14.x"
  }
}
